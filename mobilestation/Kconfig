source "Kconfig.zephyr"

config WDG_TIMEOUT_SEC
	int "Watchdoog timeout"
	default 30

config BUFF_MAX_STRING_LEN
	int "Maximum length for mqtt_broker or wlab name"
	default 32

config SIM800L_NET_WAIT_MAX_SEC
	int "Maximum number of seconds waiting for gsm network connection for sim800l modem"
	default 120